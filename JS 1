    <p id="demo"></p>
    <script>
    function is_date(d){
        return d instanceof Date
}

console.log(is_date("October 13, 2014 11:13:00"));
console.log(is_date(new Date(86400000)));
console.log(is_date(new Date(99,5,24,11,33,30,0)));
console.log(is_date([1, 2, 4, 0]));

    </script>
</body>
