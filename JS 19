    <p id="demo"></p>
    <script>
        function get_date(dob)
        {
            var d=dob.getDate()
            if(d<10)
                return ('0'+d)
            else
                return (d)
        }
        console.log(get_date(new Date(1982, 11, 4)));
        console.log(get_date(new Date(1962, 10, 10)));
        console.log(get_date(new Date(1998,4,20)));
    </script>
</body>
